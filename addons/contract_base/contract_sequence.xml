<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- 合同序列号定义 -->
        <record id="seq_type_contract_contract" model="ir.sequence.type">
            <field name="name">合同序列号</field>
            <field name="code">contract.contract</field>
        </record>

        <record id="seq_contract_contract" model="ir.sequence">
            <field name="name">合同序列号</field>
            <field name="code">contract.contract</field>
            <field name="prefix">C-%(year)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
